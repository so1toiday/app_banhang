        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.CategoryActivity"></activity>
        <activity android:name=".ui.activity.DonHangActivity" />
        <activity android:name=".ui.activity.ChangePasswordActivity" />
        <activity android:name=".ui.activity.ThongBaoActivity" />
        <activity android:name=".ui.activity.ProductActivity" />
        <activity android:name=".ui.activity.ForgetPasswrodActivity" />
        <activity android:name=".ui.activity.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.SettingActivity" />
    </application>

</manifest>